{
  "dashboard": {
    "id": null,
    "title": "GopherKit Microservices Dashboard",
    "tags": ["gopherkit", "microservices", "prometheus"],
    "style": "dark",
    "timezone": "",
    "panels": [
      {
        "id": 1,
        "title": "HTTP Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total[5m])) by (service)",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 2,
        "title": "HTTP Response Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, service))",
            "refId": "A",
            "legendFormat": "95th percentile - {{service}}"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, service))",
            "refId": "B",
            "legendFormat": "50th percentile - {{service}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 3,
        "title": "HTTP Status Codes",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total[5m])) by (status, service)",
            "refId": "A",
            "legendFormat": "{{service}} - {{status}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Database Connections",
        "type": "timeseries",
        "targets": [
          {
            "expr": "db_connections_active",
            "refId": "A",
            "legendFormat": "Active - {{service}}"
          },
          {
            "expr": "db_connections_idle",
            "refId": "B",
            "legendFormat": "Idle - {{service}}"
          },
          {
            "expr": "db_connections_max",
            "refId": "C",
            "legendFormat": "Max - {{service}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Database Query Duration",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(db_query_duration_seconds_bucket[5m])) by (le, service, operation))",
            "refId": "A",
            "legendFormat": "95th percentile - {{service}} {{operation}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Cache Hit Ratio",
        "type": "stat",
        "targets": [
          {
            "expr": "cache_hit_ratio",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 24
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 0.7
                },
                {
                  "color": "green",
                  "value": 0.9
                }
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Cache Operations",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(cache_operations_total[5m])) by (service, operation, status)",
            "refId": "A",
            "legendFormat": "{{service}} {{operation}} {{status}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 24
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Memory Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "memory_usage_bytes",
            "refId": "A",
            "legendFormat": "{{service}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 24
        },
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Business Events",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(business_events_total[5m])) by (service, event_type, tenant_id)",
            "refId": "A",
            "legendFormat": "{{service}} {{event_type}} {{tenant_id}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 32
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 10,
        "title": "JWT Token Operations",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(jwt_tokens_issued_total[5m])) by (service, token_type)",
            "refId": "A",
            "legendFormat": "Issued - {{service}} {{token_type}}"
          },
          {
            "expr": "sum(rate(jwt_tokens_validated_total[5m])) by (service, status)",
            "refId": "B",
            "legendFormat": "Validated - {{service}} {{status}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 32
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 11,
        "title": "Active Goroutines",
        "type": "timeseries",
        "targets": [
          {
            "expr": "goroutines_active",
            "refId": "A",
            "legendFormat": "{{service}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 40
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 12,
        "title": "Tenant Request Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum(rate(tenant_requests_total[5m])) by (tenant_id)",
            "refId": "A",
            "legendFormat": "{{tenant_id}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 40
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "annotations": {
      "list": [
        {
          "name": "Annotations & Alerts",
          "enable": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "type": "dashboard",
          "builtIn": 1
        }
      ]
    },
    "templating": {
      "list": [
        {
          "name": "service",
          "type": "query",
          "query": "label_values(http_requests_total, service)",
          "current": {
            "value": "All",
            "text": "All"
          },
          "includeAll": true,
          "multi": true
        },
        {
          "name": "tenant",
          "type": "query",
          "query": "label_values(http_requests_total, tenant_id)",
          "current": {
            "value": "All",
            "text": "All"
          },
          "includeAll": true,
          "multi": true
        }
      ]
    }
  }
}